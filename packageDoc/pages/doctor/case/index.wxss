.topline{
   
height: 20rpx;
background: #F5F5F5;
}
.wrap{
width: 702rpx;
margin-left: 24rpx;
}
.row {
   
    margin-bottom: 60rpx;
    overflow: hidden;
    margin-top: 40rpx;
    margin-bottom: 40rpx;
}
 .row .title {
    font-size: 32rpx;
    font-weight: bold;
    color: #1A1A1A;
    line-height: 32rpx;
}
 .row .left {
    float: left;
}
.row .right {
    float: right;
    font-size: 30rpx;
    font-weight: 400;
    color: #999999;
    line-height: 32rpx;
}
.row .right .icon {
    margin-left: 20rpx;
    color: #1A1A1A;
}

.selectbtn{
    margin-top: 30rpx;
    margin-bottom: 30rpx;
    width: 268rpx;
    margin-left: auto;
height: 68rpx;
background: #F0F7FF;
border: 1rpx solid #3894FF;
border-radius: 2rpx;
font-size: 28rpx;
color: #3894FF;
display: flex;
flex-direction: row;
align-items: center;
justify-content: center;
}


.card {
    
   
    border: 1rpx solid #E6E6E6;
    box-shadow: 0px 2rpx 4rpx 0px rgba(204,204,204,0.35);
    border-radius: 8rpx;
    display: flex;
    flex-direction: column;

    margin-bottom: 30rpx;
}


.todo-card-top {
    display: flex;
    flex-direction: row;
    align-items: center;
    padding: 18rpx 0 18rpx 20rpx;
    background: #EFF6FF;
}



.todo-title {
    
    margin-left: 20rpx;
    font-size: 28rpx;
    color: #4D4D4D;
   
}



.todo-content {
    font-size: 28rpx;
    color: #1A1A1A;
    padding: 0 40rpx 0 20rpx;
   margin-top: 30rpx;
   margin-bottom: 30rpx;
    overflow:hidden;
    text-overflow: ellipsis;
    display:-webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
}
.btn-blue {
    margin-left: auto;
    margin-right: auto;
    margin-bottom: 30rpx;
    margin-top: 26rpx;
}
.custom-class{
   
    width: 302rpx !important;
height: 58rpx !important;
background: #3894FF;
}

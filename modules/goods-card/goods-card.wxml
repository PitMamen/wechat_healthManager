<template name="chat-custom">

    <view style="border-radius: 20rpx;padding: 20rpx;font-size: 30rpx;width: 65%;display: flex; flex-direction: row;align-items: center;" bindtap="CustomArticleMessageClickEvent" data-id="{{content.data.id}}">
        <image src="{{content.data.url}}" style="width: 400rpx;height: 140rpx;margin-right: 20rpx;" />
        <view>
            {{content.data.content}}
        </view>
    </view>



</template>
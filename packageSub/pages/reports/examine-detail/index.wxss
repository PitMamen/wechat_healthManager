page{
    background-color: #F5F5F5;
}
.top {
    width: 702rpx;
    margin-left: 24rpx;
    background: #FFFFFF;
    border-radius: 8rpx;
  
    display: flex;
    flex-direction: column;
    color: #4D4D4D;
    font-size: 28rpx;
  
}

.todo-title-view {
    display: flex;
    flex-direction: row;
    align-items: center;
     background: #f5f5f5;
    padding: 30rpx 0 20rpx 24rpx;
}

.todo-title-des {
    color: #999999;
    font-size: 24rpx;
}

.split {
    height: 1rpx;
    width: 100%;
    background: #eeeeee;
}

.split-v {
    height: 100%;
    width: 1rpx;
    background: #eeeeee;
}

.todo-title {
    left: 0;
    font-size: 32rpx;

    color: #1A1A1A;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    margin-right: 20rpx;
}

.todo-title-right {
    margin-left: auto;
    display: flex;
    flex-direction: row;
    align-items: center;
    color: #999999;
    font-size: 26rpx;
}
.todo-title-tab{
    display: flex;
    flex-direction: column;
    align-items: center;
    margin-right: 30rpx;
}
.tab-title {

    font-size: 30rpx;
    color: #4D4D4D;
}

.tab-title-active {

    font-size: 36rpx;
    color: #278CFF;
}

.tab-line {
    margin-top: 12rpx;
    width: 95rpx;
    height: 4rpx;
    background: #F5F5F5;
}

.tab-line-active {
    margin-top: 12rpx;
    width: 95rpx;
    height: 4rpx;
    background: #278CFF;
}

.top-h1 {
    display: flex;
    align-items: flex-end;
    flex-direction: row;
    margin-bottom: 30rpx;
    margin-top: 30rpx;
    margin-left: 26rpx;
    margin-right: 26rpx;
    font-size: 32rpx;
    font-weight: bold;
    color: #1A1A1A;
}

.top-h2 {
    display: flex;
    align-items: flex-end;
    flex-direction: row;
    margin-bottom: 26rpx;
    margin-left: 26rpx;
}

.top-l {
    width: 50%;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
    padding-right: 10rpx;
}

.top-r {
    width: 320rpx;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}




.title-scroll {

    width: 100%;
    overflow: hidden;
    white-space: nowrap;
    padding-bottom: 20rpx;
    padding-top: 26rpx;

    display: flex;
    align-items: center;

}

.title {
    width: 168rpx;
    height: 56rpx;
    background: #F2F2F2;
    color: #141418;
    border-radius: 28rpx;
    font-size: 28rpx;
    line-height: 56rpx;
    text-align: center;
    overflow: hidden;
    margin-left: 20rpx;
    display: inline-block;
}

.active {
    color: #0379FF;
    box-sizing: border-box;
    border: 1px solid #0379FF;

}

.case-block {
    width: 702rpx;
    margin-left: 24rpx;
    margin-top: 30rpx;
    background-color: white;
}

.case-title {
    color: #1A1A1A;
    font-size: 30rpx;
}

.case-content {
    color: #999999;
    font-size: 28rpx;
    margin-top: 20rpx;
}

.case-block2 {
    padding: 26rpx;
    border-bottom: 1px solid #E6E6E6
}

.row {
  
    font-size: 24rpx;
    font-weight: bold;
    padding: 18rpx 20rpx;
    margin-top: 30rpx;
}

.li {
    padding: 29rpx 20rpx;
    color: #999999;
    font-size: 24rpx;
}

.topview{
    display: flex;
    flex-direction: column;
   
    background-color: #ffffff;
    width: 750rpx;
    height: 160rpx;
    display: flex;
    flex-direction: row;
    align-items: center;
   justify-content: space-between;
}

.topview .img{
    width: 84rpx;
    height: 88rpx;
    margin-right: 50rpx;
}
.topview .content {
    display: flex;
    flex-direction: column;
    justify-content: flex-start;
    margin-left: 50rpx;
    color: #1A1A1A;
    font-size: 32rpx;
   
}

.card{
    width: 702rpx;
margin-left: 24rpx;
background: #FFFFFF;
border-radius: 8rpx;
margin-top: 30rpx;
padding-top: 30rpx;
padding-bottom: 30rpx;
}
.cardback{
   margin-bottom: 30rpx;
}
.card .titleview{
    display: flex;
    flex-direction: row;
    align-items: center;
    margin-left:  20rpx;
    margin-bottom: 27rpx;
    margin-right: 20rpx;
}
.card .tag{
    width: 4rpx;
height: 30rpx;
background: #409EFF;
}
.card .titles{
    font-size: 32rpx;
font-family: PingFang SC;
font-weight: bold;
color: #1A1A1A;
margin-left: 20rpx;
}
.card .content{
   
font-size: 30rpx;
margin-left: 20rpx;
margin-right: 20rpx;
color: #1A1A1A;
}

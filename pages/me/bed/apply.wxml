  </view>
  <view class="content-view">
    <view>预约科室：<text style="color:#666;">{{info.cert.appointDeptName}}</text></view>
  </view>
  <view class="content-view">
    <view>申请日期：<text style="color:#666;">{{visitTime}}</text></view>

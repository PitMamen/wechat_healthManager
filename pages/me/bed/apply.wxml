    <view class="right">年龄：<text style="color:#666;">{{info.patient.userAge}}岁</text></view>
  </view>
  <view class="content-view">
    <view>身份证号：<text style="color:#666;">{{info.patient.identificationNo_x}}</text></view>
    
  </view>
